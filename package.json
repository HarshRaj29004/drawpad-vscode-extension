{
  "name": "drawpad",
  "displayName": "DrawPad",
  "description": "You can do rough work on VSCode now",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "drawpad.toggle",
        "title": "Toggle Drawing Pad",
        "icon": {
          "light": "resources/draw-icon.svg",
          "dark": "resources/draw-icon.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "editorFocus",
          "command": "drawpad.toggle",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}